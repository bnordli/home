{
  "protocols": {
    "igmp-proxy": {
      "interface": {
        "eth0": {
          "alt-subnet": ["192.168.1.0/24"],
          "role": "downstream",
          "threshold": "1"
        },
        "eth2": {
          "alt-subnet": ["0.0.0.0/0"],
          "role": "upstream",
          "threshold": "1"
        }
      }
    }
  },
  "firewall": {
    "source-validation": "disable"
  }
}
